    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "lodash-webpack-plugin": "^0.9.1",
    "mocha": "^2.5.3",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "redbox-react": "^1.2.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.4",
    "sinon-chai": "git+https://git@github.com/slightlytyler/sinon-chai.git",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-relative-loader": "^2.1.1",
